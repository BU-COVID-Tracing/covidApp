{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "@react-native-community",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:flowtype/recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended",
    "prettier/flowtype",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "react-native",
    "flowtype",
    "jest",
    "prettier"
  ],
  "rules": {
    "no-console": "error",
    "react/display-name": 0
  },
  "env": {
    "react-native/react-native": true
  }
}